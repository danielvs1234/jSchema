grammar org.xtext.example.mydsl.JSchema with org.eclipse.xtext.common.Terminals

generate jSchema "http://www.xtext.org/example/mydsl/JSchema"

Model:
	Object
;

terminal BOOLEAN:
	'True' | 'False'
;

terminal DOUBLE:
	INT'.'INT
;

Object:
	'object' (ObjectRequireMinimumProperties)?
;

ObjectRequireMinimumProperties:
	'require' (('minimum') | ('maximum')) INT 'properties' properties=hasProperties
;

Types:
	{Object} 'object' | {STRING} 'string' |	{Array} 'array' | {Boolean} 'boolean' | {Null} 'null'
;

hasProperties:
	'{' properties+=Property* '}'
;

Property:
	'name' name=ID 'type' type=Types 
;


//Types
Array:
'array''[' (length=INT)? ']'
;










